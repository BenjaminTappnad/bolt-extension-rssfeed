{
    "name": "bolt/rssfeed",
    "description": "RSS feeds extension for Bolt",
    "type": "bolt-extension",
    "keywords": [
        "RSS"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\RssFeed\\": [
                "src/"
            ]
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bolt\\RssFeed\\RssFeedExtension",
        "bolt-icon": "icon.png",
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        }
    }
}
